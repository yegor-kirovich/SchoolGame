if info == "DMG+"{
	sprite_index = Sprite_ButtonDMG
}
else if info == "-CDHit"{
	sprite_index = Sprite_CDHit
}
else if info == "+Miss"{
	sprite_index = Sprite_Miss
}
else if info == "-CDDash"{
	sprite_index = Sprite_CDDash
}
else if info == "DashInv"{
	sprite_index = Sprite_InvDash
}
else if info == "SlowD"{
	sprite_index = Sprite_SlowD
}
else if info == "DoubleHit"{
	sprite_index = Sprite_DoubleHit
}
else if info == "Dash+Hit"{
	sprite_index = Sprite_DashHit
}